# Seed Script Configuration
# Copy to ../../.env or set as environment variables

# Database Connection (required)
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/app_db
POSTGRES_URL=postgresql://postgres:postgres@localhost:5432/app_db

# OpenSearch Connection (optional - will skip if unavailable)
OPENSEARCH_URL=http://localhost:9200
OPENSEARCH_USERNAME=admin
OPENSEARCH_PASSWORD=admin

# Seed Behavior
RESET_DATA=false                    # Set to true to truncate tables before seeding

# Data Volume Configuration
ORGS_COUNT=5                        # Number of demo organizations to create
USERS_PER_ORG=3                     # Users per organization
BUSINESSES_PER_INDUSTRY=50          # Businesses per industry (10 industries total)
CONTACTS_PER_BUSINESS=2             # Contacts per business
SOCIAL_PROFILES_PROB=0.6            # Probability (0-1) of business having social profiles
LEAD_LISTS_PER_USER=2               # Lead lists per user
ITEMS_PER_LEAD_LIST=25              # Businesses per lead list
SAVED_SEARCHES_PER_USER=3           # Saved searches per user
ALERTS_PER_USER=2                   # Alerts per user
ICP_CONFIGS_PER_ORG=2               # ICP configurations per organization

# Example Presets:

# Small Dataset (for quick testing)
# ORGS_COUNT=2
# USERS_PER_ORG=2
# BUSINESSES_PER_INDUSTRY=20
# CONTACTS_PER_BUSINESS=1

# Medium Dataset (default - good for demos)
# ORGS_COUNT=5
# USERS_PER_ORG=3
# BUSINESSES_PER_INDUSTRY=50
# CONTACTS_PER_BUSINESS=2

# Large Dataset (for load testing)
# ORGS_COUNT=20
# USERS_PER_ORG=5
# BUSINESSES_PER_INDUSTRY=200
# CONTACTS_PER_BUSINESS=3
