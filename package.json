{
  "name": "engine-labs-api",
  "version": "0.0.1",
  "description": "Engine Labs API Platform",
  "author": "Engine Labs Team",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:vitest": "vitest",
    "test:vitest:ui": "vitest --ui",
    "test:pw": "playwright test",
    "db:migrate": "typeorm migration:run -d dist/database/data-source.js",
    "db:migrate:generate": "typeorm migration:generate",
    "db:migrate:revert": "typeorm migration:revert -d dist/database/data-source.js"
  },
  "dependencies": {
    "@nestjs/apollo": "^12.0.0",
    "@nestjs/common": "^10.2.0",
    "@nestjs/config": "^3.1.0",
    "@nestjs/core": "^10.2.0",
    "@nestjs/graphql": "^12.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.2.0",
    "@nestjs/typeorm": "^9.0.0",
    "@opensearch-project/opensearchjs": "^2.3.0",
    "@types/express": "^4.17.17",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "geocoding": "^0.4.0",
    "graphql": "^16.8.0",
    "node-geocoder": "^4.2.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.0",
    "rxjs": "^7.8.0",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.2.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.2.0",
    "@playwright/test": "^1.40.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.0",
    "vitest": "^1.1.0"
  }
}
